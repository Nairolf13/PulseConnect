model Assets {
  id               Int                @id @default(autoincrement())
  name             String             @unique @db.VarChar(255)
  genre            Genre
  description      String?            @db.VarChar(255)
  likes            Likes[]
  commentaire      Commentaires[]
  id_user          Int
  id_project       Int?
  isPublic         Boolean?           @default(true)
  price            Decimal?           @db.Decimal(10, 2)
  url              String?            @db.VarChar(255)
  created_at       DateTime?          @default(now()) @db.DateTime(0)
  Users            Users?             @relation(fields: [id_user], references: [id_user], onDelete: NoAction, onUpdate: NoAction, map: "Assets_ibfk_1")
  Projects         Projects?          @relation(fields: [id_project], references: [id_project], onDelete: NoAction, onUpdate: NoAction, map: "Assets_ibfk_2")
  AssetsToCommande AssetsToCommande[]

  @@index([id_project], map: "id_project")
  @@index([id_user], map: "id_user")
}

