model Commandes {
  id               Int                @id @default(autoincrement())
  id_user          Int?
  payment_status   String?            @default("pending") @db.VarChar(50)
  amount           Decimal?           @db.Decimal(10, 2)
  platform_fee     Decimal?           @db.Decimal(5, 2)
  created_at       DateTime?          @default(now()) @db.DateTime(0)
  AssetsToCommande AssetsToCommande[]
  Users            Users?             @relation(fields: [id_user], references: [id_user], onDelete: NoAction, onUpdate: NoAction, map: "Commandes_ibfk_1")

  @@index([id_user], map: "id_user")
}