model UsersToProjects {
  id         Int                  @id @default(autoincrement())
  id_user    Int
  id_project Int?
  role       UsersToProjects_role
  created_at DateTime?            @default(now()) @db.DateTime(0)
  Users      Users?               @relation(fields: [id_user], references: [id_user], onDelete: NoAction, onUpdate: NoAction, map: "UsersToProjects_ibfk_1")
  Projects   Projects?            @relation(fields: [id_project], references: [id_project], onDelete: NoAction, onUpdate: NoAction, map: "UsersToProjects_ibfk_2")

  @@index([id_project], map: "id_project")
  @@index([id_user], map: "id_user")
}

enum UsersToProjects_role {
  owner
  user
}
